    "author": "Jarvis Prestidge (jarvisprestidge@gmail.com)",
    "license": "MIT",
    "dependencies": {
        "bunyan": "^1.8.12"
    },
    "devDependencies": {
        "@types/bunyan": "^1.8.4",
        "@types/node": "^10.0.4",
        "tslint": "^5.9.1",
        "tslint-config-prettier": "^1.10.0",
        "typescript": "^2.8.1"
    }
}
