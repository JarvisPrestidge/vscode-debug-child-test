{
    "name": "flobby",
    "version": "1.0.1",
    "description": "An electron template leveraging typescript, marko, lasso and bulma",
    "main": "dist/main/main.js",
    "scripts": {
        "tsc": "tsc -p .",
        "clean": "rm -rf dist",
        "build": "npm run clean && npm run tsc",
        "start": "node dist/main.js"
    },
    "keywords": [
        "typescript",
